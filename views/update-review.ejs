<%- include('./partials/header') %>

<section id="update-review-page" class="container mt-4">
<div class="card-body">
  <h2 class="card-title">Update Review</h2>
  <form id="update-review-form" method="POST" action="/prints/<%= print._id %>/reviews/<%= review._id %>/update">
    <div class="form-group">
      <textarea class="form-control" name="content"><%= review.content %></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Update Review</button>
  </form>
</div>
</section>

<%- include('./partials/footer') %>